{
  "name": "fps.ts",
  "version": "0.18.4",
  "description": "JavaScript Performance Monitor - TypeScript Edition",
  "main": "build/fps.js",
  "module": "build/fps.module.js",
  "types": "build/Stats.d.ts",
  "exports": {
    ".": {
      "types": "./build/Stats.d.ts",
      "import": "./build/fps.module.js",
      "require": "./build/fps.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronbeall/fps.ts.git"
  },
  "files": [
    "build/*.js",
    "build/*.d.ts",
    "build/*.map",
    "src/**/*.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "performance",
    "fps",
    "stats",
    "monitor",
    "typescript",
    "browser",
    "webgl",
    "animation"
  ],
  "author": "aaronbeall <aaronbeall@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aaronbeall/fps.ts/issues"
  },
  "homepage": "https://github.com/aaronbeall/fps.ts#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^2.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
